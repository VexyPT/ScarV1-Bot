{
  "$schema": "http://json.schemastore.org/package",
  "name": "scarbot",
  "type": "module",
  "main": "dist/index.js",
  "author": "vexy",
  "scripts": {
    "build": "tsc",
    "dev": "tsx --env-file .env ./src/index.ts",
    "canary": "tsx --env-file .env.development ./src/index.ts",
    "watch": "tsx --watch --env-file .env ./src/index.ts",
    "watch:canary": "tsx --watch --env-file .env.development ./src/index.ts",
    "start": "node --env-file .env .",
    "start:canary": "node --env-file .env.development ."
  },
  "dependencies": {
    "@magicyan/discord": "^1.0.24",
    "@squarecloud/api": "^3.4.3",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "discord.js": "^14.14.1",
    "fast-glob": "^3.3.2",
    "mongoose": "^8.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "imports": {
    "#base": [
      "./dist/discord/base/index.js"
    ],
    "#settings": [
      "./dist/settings/index.js"
    ],
    "#database": [
      "./dist/database/index.js"
    ],
    "#functions": [
      "./dist/functions/index.js"
    ],
    "#classes": [
      "./dist/classes/index.js"
    ],
    "#tools": [
      "./dist/tools/index.js"
    ],
    "#menus": [
      "./dist/menus/index.js"
    ]
  }
}
